{
  "image": "public.ecr.aws/dev1-sg/devcontainer/ubuntu-base:latest",
  "name": "docker-ci-images",
  "features": {
      "ghcr.io/dev1-sg/devcontainers/common-utils:1": {},
      "ghcr.io/dev1-sg/devcontainers/docker-in-docker:latest": {},
      "ghcr.io/dev1-sg/devcontainers/aws-cli:1": {},
      "ghcr.io/dev1-sg/devcontainers/github-cli:1": {}
  },
  "customizations": {
    "vscode": {
      "settings": {"terminal.integrated.defaultProfile.linux": "zsh"},
    "extensions": ["ms-vscode-remote.remote-containers", "golang.Go", "ms-python.python"]
    }
  },
  "containerUser": "vscode",
  "workspaceMount": "source=${localWorkspaceFolder}/,target=/home/vscode/docker-ci-images,type=bind,consistency=cached", "workspaceFolder": "/home/vscode",
  "postCreateCommand": "sudo chown -R vscode:vscode /home/vscode"
}
